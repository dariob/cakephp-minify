{
    "name": "visonforcoding/cakeminify",
    "description": "Cakeminify plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0",
        "matthiasmullie/minify":"*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Cakeminify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cakeminify\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
